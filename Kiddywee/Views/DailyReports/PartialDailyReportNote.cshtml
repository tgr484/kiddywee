@model List<DailyReportNoteViewModel>
@{ 
    Layout = null;
}
<div class="text-right">
    <button id="add-note" data-url="@Url.Action("AddEditNote", "DailyReports")" class="btn btn-info waves-effect"><i class="sl-icon-plus"></i> Add report</button>
</div>
<table class="table color-table info-table">
    <tbody id="report-table">
        @{
            foreach (var n in Model)
            {
                <tr id="@n.Id">
                    <td>@n.Note</td>
                    <td><a href="#" class="delete-note" data-url="@Url.Action("DeleteNote", "DailyReports", new { id = n.Id} )">Delete</a></td>
                </tr>
            }
        }
    </tbody>
</table>