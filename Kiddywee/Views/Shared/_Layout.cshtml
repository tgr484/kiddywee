    <script>
        $(document).ready(function () {
            $('.owl-carousel').owlCarousel({
                loop: false,
                margin: 10,
                nav: true,
                items: 5,
            })

            $(document).on("click", ".add-class", function () {
                var url = $(this).data("url");
                $.get(url, function (data) {
                    $(".modal-content").html(data)
                    $("body").addClass("modal-open");
                    $(".modal").show();
                })
            });
        })
